import sys
import serial
import time
from struct import *;


ser = serial.Serial("COM3",timeout=5)  #open first serial port
print ser.portstr       #check which port was really used

buf=''
buf+=pack('c', 'AT')
buf+=pack('I', rpm)
ser.write(buf)

n=ser.inWaiting()
if n>0:
    r_buf=ser.read(n)
    print "read:"+r_buf



ser.close()